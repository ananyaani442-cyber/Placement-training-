#include <stdio.h>

int main() {
    int a, b = 0, c;

    printf("Enter a number: ");
    scanf("%d", &a);

    c = a;  

    while (a > 0) {
        b = b * 10 + (a % 10);  
        a = a / 10;
    }

    if (c == b)
        printf("Number is palindrome");
    else
        printf("Number is not palindrome");

    return 0;
}
